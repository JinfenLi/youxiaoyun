package com.topview.multimedia.vo.result;

import java.util.List;

import com.topview.multimedia.vo.AccountInfo;

public class AccountInfoResult {
	private int code;
	private boolean success;
	private List<AccountInfo> infoResult;
	public int getCode() {
		return code;
	}
	public void setCode(int code) {
		this.code = code;
	}
	public boolean isSuccess() {
		return success;
	}
	public void setSuccess(boolean success) {
		this.success = success;
	}
	public List<AccountInfo> getInfoResult() {
		return infoResult;
	}
	public void setInfoResult(List<AccountInfo> infoResult) {
		this.infoResult = infoResult;
	}


	
}
