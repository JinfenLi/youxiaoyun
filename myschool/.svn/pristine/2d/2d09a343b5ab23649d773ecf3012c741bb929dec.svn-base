package com.topview.school.vo.curricula.result;

import java.util.List;

import com.topview.school.vo.curricula.ClassCurriculaInfo;

public class ClassCurriculaInfoResult {
	
	private List<ClassCurriculaInfo> result;	
	private String comment; //课表备注
	private int code;
	private boolean success;
	private String variableId;
	
	public List<ClassCurriculaInfo> getResult() {
		return result;
	}
	public void setResult(List<ClassCurriculaInfo> result) {
		this.result = result;
	}
	public int getCode() {
		return code;
	}
	public void setCode(int code) {
		this.code = code;
	}
	public boolean isSuccess() {
		return success;
	}
	public void setSuccess(boolean success) {
		this.success = success;
	}
	public String getComment() {
		return comment;
	}
	public void setComment(String comment) {
		this.comment = comment;
	}
	public String getVariableId() {
		return variableId;
	}
	public void setVariableId(String variableId) {
		this.variableId = variableId;
	}
	
}
