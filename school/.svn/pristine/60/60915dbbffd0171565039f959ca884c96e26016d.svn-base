package com.topview.school.vo.leave.result;

import java.util.List;

import com.topview.school.vo.leave.LeaveInfo;

public class LeaveInfoResult {

	private List<LeaveInfo> result;
	private int code;
	private boolean success;

	public List<LeaveInfo> getResult() {
		return result;
	}

	public void setResult(List<LeaveInfo> result) {
		this.result = result;
	}

	public int getCode() {
		return code;
	}

	public void setCode(int code) {
		this.code = code;
	}

	public boolean isSuccess() {
		return success;
	}

	public void setSuccess(boolean success) {
		this.success = success;
	}

}
